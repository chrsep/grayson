{
  "name": "grayson-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "start": "svelte-kit start --host",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate-prod": "prisma migrate deploy",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:seed": "prisma db seed --preview-feature",
    "check": "svelte-check",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged",
    "test": "npx folio --param browserName=chromium --param screenshotOnFailure",
    "test:debug": "PWDEBUG=1 npx folio --param browserName=chromium --param screenshotOnFailure --timeout 0",
    "ts-node": "node --loader ts-node/esm "
  },
  "devDependencies": {
    "@playwright/test": "0.1100.0",
    "@prisma/client": "2.20.1",
    "@sveltejs/adapter-node": "1.0.0-next.12",
    "@sveltejs/kit": "1.0.0-next.71",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.3.2",
    "@tailwindcss/typography": "0.4.0",
    "@types/node": "14.14.37",
    "@types/node-fetch": "2.5.9",
    "@types/pino": "6.3.6",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "argon2": "0.27.2",
    "autoprefixer": "10.2.5",
    "cssnano": "4.1.10",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-svelte3": "3.1.2",
    "husky": "6.0.0",
    "jsonwebtoken": "8.5.1",
    "nanoid": "^3.1.22",
    "pino": "6.11.2",
    "playwright": "1.10.0",
    "postcss": "8.2.9",
    "postcss-load-config": "3.0.1",
    "prettier": "2.2.1",
    "prettier-plugin-prisma": "0.7.0",
    "prettier-plugin-svelte": "2.2.0",
    "pretty-quick": "3.1.0",
    "prisma": "2.20.1",
    "rollup": "2.44.0",
    "slugify": "1.5.0",
    "svelte": "3.37.0",
    "svelte-check": "1.3.0",
    "svelte-preprocess": "4.7.0",
    "tailwindcss": "2.1.1",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.2.3",
    "uuid": "8.3.2",
    "vite": "2.1.5",
    "yup": "0.32.9"
  },
  "dependencies": {
    "@types/cookie": "0.4.0",
    "@types/jsonwebtoken": "8.5.1",
    "dayjs": "1.10.4",
    "svelte-feather-icons": "3.3.0"
  },
  "type": "module"
}
